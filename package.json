        "@typescript-eslint/eslint-plugin": "4.32.0",
        "@typescript-eslint/parser": "4.32.0",
        "dpdm": "3.8.0",
        "eslint": "7.32.0",
        "eslint-config-alloy": "4.3.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.2",
